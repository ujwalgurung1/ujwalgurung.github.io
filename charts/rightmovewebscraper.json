{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": {
        "text": "Listed 3 Bed Homes in Bristol on Rightmove",
        "subtitle":"Price, Source: Rightmove",
        "subtitleFontStyle":"italic",
        "subtitleFontSize":10,
        "anchor": "start",
        "color": "black"
      },
    "data": {
        "url": "https://raw.githubusercontent.com/ujwalgurung1/ujwalgurung1.github.io/main/data/Actual.csv",
        "format": {"type": "csv"}
      },
    "height": 280,
    "width": 800,
    "layer": [{
      "mark": "bar",
      "encoding": {
        "x": {"field": "price", "bin": true, "title": null},
        "y": {"aggregate": "count", "title": "Number of Homes"}
      }
    },{
      "mark": "rule",
      "encoding": {
        "x": {"aggregate": "median", "field": "price"},
        "color": {"value": "red"},
        "size": {"value": 5}
      }
    }]
  }
  